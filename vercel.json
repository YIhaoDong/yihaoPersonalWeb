{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1",
      "has": [
        {
          "type": "host",
          "value": "card.yihaodong.me"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/card",
      "headers": {
        "host": "card.yihaodong.me"
      }
    }
  ]
}
